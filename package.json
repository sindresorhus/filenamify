{
	"name": "filenamify",
	"version": "7.0.0",
	"description": "Convert a string to a valid safe filename",
	"license": "MIT",
	"repository": "sindresorhus/filenamify",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./index.js"
		},
		"./browser": {
			"types": "./filenamify.d.ts",
			"default": "./filenamify.js"
		}
	},
	"sideEffects": false,
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"test": "xo && ava"
	},
	"files": [
		"filenamify-path.d.ts",
		"filenamify-path.js",
		"filenamify.d.ts",
		"filenamify.js",
		"index.d.ts",
		"index.js"
	],
	"keywords": [
		"filename",
		"safe",
		"sanitize",
		"file",
		"name",
		"string",
		"path",
		"filepath",
		"convert",
		"valid",
		"dirname"
	],
	"dependencies": {
		"filename-reserved-regex": "^4.0.0"
	},
	"devDependencies": {
		"ava": "^6.4.1",
		"xo": "^1.2.2"
	}
}
